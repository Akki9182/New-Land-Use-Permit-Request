        <name>auto populate comapay details</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onLoad() {
    var userSysId = g_user.userID;
    
    // Call the Script Include via GlideAjax
    var ga = new GlideAjax('UserCompanyDetails');
    ga.addParam('sysparm_name', 'getCompanyDetails');
    ga.addParam('sysparm_user_id', userSysId);
    
    ga.getXMLAnswer(function(response) {
        var result = JSON.parse(response);
        
        // Populate the form fields
        if (result.company) {
            g_form.setValue('requested_by_sector', result.company);
        }
        if (result.companyHead) {
            g_form.setValue('sector_head', result.companyHead);
        }
        if (result.headEmail) {
            g_form.setValue('sector_head_email', result.headEmail);
        }
    });
}
]]></script>
