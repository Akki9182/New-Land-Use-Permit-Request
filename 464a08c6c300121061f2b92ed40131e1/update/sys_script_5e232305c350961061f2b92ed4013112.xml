        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Construct the encoded query string for tasks that are not in a closed state (assuming state 3 is 'Closed')
    var encodedQuery = 'lup_request=' + current.lup_request +
        '^short_descriptionINGIS Validation,Document Control Quality Check' +
        '^stateIN-5,1,2'; // Replace with actual state values that are considered not closed

    var grTask = new GlideRecord('x_1329978_new_la_0_lup_task_table');
    grTask.addEncodedQuery(encodedQuery);
    grTask.query();

    var notClosedTasks = [];

    // Iterate over the results to collect tasks that are not closed
    while (grTask.next()) {
        var taskDescription = grTask.getValue('short_description'); // Get the short description
        if (taskDescription) {
            notClosedTasks.push(taskDescription);
        }
    }

    // If there are tasks that are not closed, display an error message and abort the action
    if (notClosedTasks.length > 0) {
        var errorMessage = 'You cannot close this task until the following tasks are closed: ' + notClosedTasks.join(', ') + '.';
        gs.addErrorMessage(errorMessage);
        current.setAbortAction(true); // Prevent the current task from being closed
    }

})(current, previous);]]></script>
